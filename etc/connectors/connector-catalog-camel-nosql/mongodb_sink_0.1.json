        },
        "mongodb_create_collection" : {
          "title" : "Collection",
          "description" : "Create collection during initialisation if it doesn't exist.",
          "type" : "boolean",
          "default" : false
        },
        "kafka_topic" : {
          "title" : "Topic Names",
          "description" : "Comma separated list of Kafka topic names",
          "type" : "string"
        },
        "processors" : {
          "type" : "array",
          "items" : {
            "oneOf" : [ {
